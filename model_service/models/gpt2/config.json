{
    "type": "gpt2",
    "path": "/scratch/models/gpt2",
    "parameters": {
        "temperature": {
            "type": "float",
            "default": 0.7,
            "description": "Temperature of the sampling distribution."
        },
        "top_k": {
            "type": "int",
            "default": 40,
            "description": "Number of top tokens to consider for sampling."
        },
        "top_p": {
            "type": "float",
            "default": 0.9,
            "description": "Cumulative probability of top tokens to consider for sampling."
        },
        "max_length": {
            "type": "int",
            "default": 20,
            "description": "Maximum length of the generated text."
        },
        "repetition_penalty": {
            "type": "float",
            "default": 1.0,
            "description": "Penalty for repeating tokens."
        },
        "num_return_sequences": {
            "type": "int",
            "default": 1,
            "description": "Number of sequences to return."
        },
        "length_penalty": {
            "type": "float",
            "default": 1.0,
            "description": "Penalty for length of the generated text."
        },
        "no_repeat_ngram_size": {
            "type": "int",
            "default": 0,
            "description": "Size of n-grams to avoid repeating."
        }
    }
}
